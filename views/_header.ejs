<header>
    Page Header
    <% if(locals.user){ %>
    <ul>
        <li><a href = "/">Home</a></li>
        <li><a href = "/users/profile/<%= locals.user.id %>"><%= locals.user.name %></a></li>
        <li><a href="/users/sign-out">Sign-out</a></li>
    </ul>
    <% }else{ %>
    <ul>
        <li><a href = "/users/signin">Sign In</a></li>
        <li><a href = "/users/signup">Sign Up</a></li>
    </ul>
    <% } %>
</header>